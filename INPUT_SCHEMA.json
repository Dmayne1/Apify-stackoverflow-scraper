{
  "title": "Scraper Input",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "startUrls": {
      "title": "Start URLs",
      "type": "array",
      "description": "URLs to scrape",
      "editor": "requestListSources",
      "default": [{"url": "https://example.com"}]
    },
    "maxItems": {
      "title": "Max items",
      "type": "integer",
      "description": "Maximum number of items to scrape",
      "default": 10,
      "minimum": 1
    }
  },
  "required": ["startUrls"]
}